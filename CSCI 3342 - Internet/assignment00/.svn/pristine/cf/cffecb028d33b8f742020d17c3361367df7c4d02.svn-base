$("button").click( function()
	{
    $("body").css("background-color", "LightBlue");
	} );

$("#main-img").mouseover( function()
  {
    var name = $('#main-img').attr('src');
    console.log("Before: " + name);
    if (name == "jjba.jpg") {
      name = "jjba-2.jpg";
    }
    else {
      name = "jjba.jpg";
    }
    console.log("After: " + name);

    $("#main-img").attr("src", name);

  }
);
